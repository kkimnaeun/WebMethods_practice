CREATE USER TEST_KNE IDENTIFIED BY TEST_KNE; -- user생성 

ALTER SESSION SET "_ORACLE_SCRIPT" = TRUE; -- ORA-65048 오라클 오류 발생시, 사용하는 쿼리문 

GRANT DBA, CONNECT, RESOURCE TO TEST_KNE; -- 권한 생성

CREATE TABLE TEST_KNE.SRC_EMPOLOYEES_TABLE ( -- SOURCE TABLE 생성, primary키 부여
	ID NUMBER(38,0) NOT NULL,
	FIRST_NAME VARCHAR2(100) NULL,
	LAST_NAME VARCHAR2(100) NULL,
	EMAIL VARCHAR2(100) NULL,
	GENDER VARCHAR2(100) NULL,
	EAI_FLAG VARCHAR2(100) NULL,
	CONSTRAINT SRC_EMPOLOYEES_TABLE_PK PRIMARY KEY (ID)
);

CREATE TABLE TEST_KNE.TGT_EMPOLOYEES_TABLE ( -- Target TABLE 생성, primary키 부여
	ID NUMBER(38,0) NOT NULL,
	FIRST_NAME VARCHAR2(100) NULL,
	LAST_NAME VARCHAR2(100) NULL,
	EMAIL VARCHAR2(100) NULL,
	GENDER VARCHAR2(100) NULL,
	EAI_RESULTS VARCHAR2(100) NULL,
	CONSTRAINT TGT_EMPOLOYEES_TABLE_PK PRIMARY KEY (ID)
);




